{
  "manifest_version": 2,

  "name": "chrome-t",
  "description": "Quickly navigate through open tabs from the keyboard",
  "version": "1.2",

  "icons": {
    "48": "icons/web/ic_view_list_black_48dp_1x.png",
    "96": "icons/web/ic_view_list_black_48dp_2x.png"
  },

  "permissions": [ "tabs" ],

  "browser_action": {
    "default_popup": "popup.html"
  },

  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+T"
      }
    }
  }
}
